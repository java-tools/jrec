/**
 *  Sales File
 */
@namespace("net.sf.RecordEditor.avro.Test.Sales5")

protocol sales5 {
  enum SaleType {
    RETURN, OTHER, SALE
  }
  record Product {
    int keycode;
    array<int> saleDate;
    array<int> quantity;
    array<long> price;
    array<SaleType> saleType;
  } 

  record Order {
    int keycode;
    int quantity;
  }

  record Summary {
    int quantity;
    long price;
    int count;
  }

  record Deptartment {
    int department;
    string name;
    array<Product> product;
    union { null, Summary } summary;
  }

  record Store {
    int store ;
    string name ;
    array<Deptartment> department;
    array<Order> order;
    union { null, Summary } summary;
  }
}
